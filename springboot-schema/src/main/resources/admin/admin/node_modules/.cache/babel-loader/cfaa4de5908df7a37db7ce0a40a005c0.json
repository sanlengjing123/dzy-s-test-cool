\r\n        <div v-else>\r\n          <el-form-item v-if=\"ruleForm.tupian\" label=\"采集图片\" prop=\"tupian\">\r\n            <img style=\"margin-right:20px;\" v-bind:key=\"index\" v-for=\"(item,index) in ruleForm.tupian.split(',')\" :src=\"item\" width=\"100\" height=\"100\">\r\n          </el-form-item>\r\n        </div>\r\n      </el-col>\r\n                                                                                          </el-row>\r\n                                                                                                                                                      <el-row>\r\n            <el-col :span=\"24\">\r\n              <el-form-item v-if=\"type!='info'\" label=\"病历描述\" prop=\"gangweiyaoqiu\">\r\n                <el-input\r\n                  style=\"min-width: 200px; max-width: 600px;\"\r\n                  type=\"textarea\"\r\n                  :rows=\"8\"\r\n                  placeholder=\"病历描述\"\r\n                  v-model=\"ruleForm.gangweiyaoqiu\">\r\n                </el-input>\r\n              </el-form-item>\r\n              <div v-else>\r\n                <el-form-item v-if=\"ruleForm.gangweiyaoqiu\" label=\"病历描述\" prop=\"gangweiyaoqiu\">\r\n                    <span>{{ruleForm.gangweiyaoqiu}}</span>\r\n                </el-form-item>\r\n              </div>\r\n            </el-col>\r\n          </el-row>\r\n                                                                                                                                                                                                                            <el-row>\r\n            <el-col :span=\"24\">\r\n              <el-form-item v-if=\"type!='info'\"  label=\"用药情况\" prop=\"gongzuozhize\">\r\n                <editor \r\n                    style=\"min-width: 200px; max-width: 600px;\"\r\n                    v-model=\"ruleForm.gongzuozhize\" \r\n                    class=\"editor\" \r\n                    action=\"file/upload\">\r\n                </edit